pipeline{
    
    agent any 
    
    stages {
        
        stage('Git Checkout'){
            
            steps{
                
                script{
                    
                    git branch: 'main', url: 'https://github.com/ShrutiDhanuka/devopsDemo.git'
                }
            }
        }
       
       
            stage('Quality Gate Status'){
                
                steps{
                 
                    script{
                     withSonarQubeEnv(credentialsId: 'sonar-api') {
                        
                        waitForQualityGate abortPipeline: false, credentialsId: 'sonar-api'
                    }
                    }
                }
            }
        }
        
}
